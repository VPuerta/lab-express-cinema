
<div class="oneMovie">
    <div>
<img src="{{movie.image}}" alt="image">
</div>
<div class="oneText">
    <a href="/movie">Go Back</a>
<h4>{{movie.title}}</h4>
<h3>Director: {{movie.director}}</h3>
<h5>Stars</h5>
{{#each movie.stars}}
<li>{{this}}</li>
{{/each}}

<p>{{movie.description}}</p>

{{#each movie.showtimes}}
<li>{{this}}</li>
{{/each}}
</div>

</div>



